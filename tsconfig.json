{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "lib": ["es2015", "dom"],
    "jsx": "react",
    "rootDir": "./",
    "strict": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true
  }
}
